@using Microsoft.AspNetCore.Identity
@model TechStore.ViewModels.EditSorageView
@{
    ViewBag.Title = "Edit storage";
}

<form asp-action="Edit" asp-controller="Storage">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group row">
        <div class="col">
            <input type="hidden" asp-for="Id" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col">
            <label asp-for="City" class="control-label">City</label>
            <input type="text" asp-for="City" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col">
            <label asp-for="Street" class="control-label">Street</label>
            <input type="text" asp-for="Street" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col">
            <button type="submit" value="Save" class="btn btn-primary">Save</button>
        </div>
    </div>
</form>