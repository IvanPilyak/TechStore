@using Microsoft.AspNetCore.Identity
@model TechStore.ViewModels.EditProducerView
@{
    ViewBag.Title = "Edit producer";
}

<form asp-action="Edit" asp-controller="Producer">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group row">
        <div class="col">
            <input type="hidden" asp-for="Id" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col">
            <label asp-for="Name" class="control-label">Name</label>
            <input type="text" asp-for="Name" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col">
            <label asp-for="Phone" class="control-label">Phone</label>
            <input type="text" asp-for="Phone" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col">
            <label asp-for="Email" class="control-label">Email</label>
            <input type="text" asp-for="Email" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col">
            <label asp-for="WebSite" class="control-label">WebSite</label>
            <input type="text" asp-for="WebSite" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col">
            <button type="submit" value="Save" class="btn btn-primary">Save</button>
        </div>
    </div>
</form>